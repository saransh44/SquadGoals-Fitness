<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:id="@+id/StartUpPage">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/imageView3"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@mipmap/topofhomescreen"
        android:layout_above="@+id/wall" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/wall"
        android:background="@mipmap/wall"
        android:layout_alignWithParentIfMissing="false"
        android:layout_above="@+id/imageView5"
        android:layout_centerHorizontal="true"
        android:minHeight="39dp" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/imageView5"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@mipmap/bottomofhomescreen"
        android:minHeight="80dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Log_In"
        android:background="@mipmap/loginbutton"
        android:layout_alignTop="@+id/wall"
        android:layout_toLeftOf="@+id/wall"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:minHeight="40dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Sign_Up"
        android:background="@mipmap/signup"
        android:layout_alignTop="@+id/wall"
        android:layout_toRightOf="@+id/wall"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:minWidth="0dp"
        android:minHeight="40dp" />
</RelativeLayout>
